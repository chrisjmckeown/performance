<div class="wrapper">
  {{> header}}
  <main class="main">
    <div class="uk-container">
      {{> navigation/primary-nav}}
      <div class="uk-grid">
        <div class="uk-width-4-5">
          <div class="uk-box-shadow-xlarge uk-padding">
            <legend class="uk-legend">Members</legend>
            <p>Edit, delete, and view members details including recipes and playlists.</p>
            <ul class="uk-list uk-list-divider uk-list-striped">
              {{#each User}}
              {{> row-item text=this.dataValues.userName}}
              {{/each}}
            </ul>
          </div>
        </div>
        <div class="uk-width-1-5">
          {{> chat}}
        </div>
      </div>
    </div>
  </main>
  {{> footer}}
</div>
<!-- App scripts and css-->
<script src="/js/admin-users.js"></script>